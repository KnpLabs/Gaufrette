{
    "name":         "knplabs/gaufrette",
    "type":         "library",
    "description":  "PHP5 library that provides a filesystem abstraction layer",
    "keywords":     ["file", "filesystem", "media", "abstraction"],
    "homepage":     "http://knplabs.com",
    "license":      "MIT",
    "authors": [
        {
            "name":     "Antoine HÃ©rault",
            "email":    "antoine.herault@gmail.com"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/knplabs/Gaufrette/contributors"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url":  "pear.amazonwebservices.com"
        },
        {
            "type": "package",
            "package": {
                "name": "rackspace/php-cloudfiles",
                "version": "master",
                "source": {
                    "url":  "git://github.com/rackspace/php-cloudfiles.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/rackspace/php-cloudfiles/zipball/master",
                    "type": "zip"
                }
            }
        },
        {
            "type": "pear",
            "url": "pear.dropbox-php.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "pear-aws/sdk": "*",
        "rackspace/php-cloudfiles": "*",
        "doctrine/dbal": ">=2.3",
        "pear-dropbox-php/Dropbox": "*"
    },
    "suggest": {
        "knplabs/knp-gaufrette-bundle": "*",
        "pear-aws/sdk":                 ">=1.4.5",
        "rackspace/php-cloudfiles":     "*",
        "doctrine/dbal":                ">=2.3",
        "ext-curl":                     "*",
        "ext-mbstring":                 "*",
        "ext-mongo":                    "*"
    },
    "autoload": {
        "psr-0": { "Gaufrette": "src/" }
    }
}
